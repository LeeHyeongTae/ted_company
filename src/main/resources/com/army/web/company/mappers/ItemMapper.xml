<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.army.web.company.mappers.ItemMapper">
    <select id="selectAll" resultType="com.army.web.company.domains.ItemDTO">
        select
            weapon_number weaponNumber,
            weapon_name weaponName,
            personal,
            common,
            condition,
            speciality
        from item
    </select>
</mapper>