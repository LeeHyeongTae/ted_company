<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.army.web.company.mappers.MemberMapper">
    <select id="selectAll" resultType="com.army.web.company.domains.MemberDTO">
        select
            name,
            email,
            password
            from member
    </select>
    <insert id="insertMember" parameterType="com.army.web.company.domains.MemberDTO">
        insert into from member values
    </insert>
</mapper>